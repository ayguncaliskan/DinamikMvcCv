@model MvcCv.Models.Entity.TblSertifikalarım
@{
    ViewBag.Title = "SertifikaGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Sertifika Güncelleme Sayfası</h4>
<br />
<form method="post">

    @Html.Label("Sertifika Id")
    @Html.TextBoxFor(x => x.ID, new { @class = "form-control" })
    <br />
    @Html.Label("Sertifika Adı")
    @Html.TextBoxFor(x => x.Aciklama, new { @class = "form-control" })
    <br />
    @Html.Label("Sertifika Linki")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
   
    <button class="btn btn-info">Güncelle</button>
</form>

