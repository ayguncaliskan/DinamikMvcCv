@using MvcCv.Models.Entity
@model List<TblEgitim>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Egitim Listesi</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Okul Adı</th>
        <th>Fakülte</th>
        <th>Bölüm</th>
        <th>Gno</th>
        <th>Tarih</th>
        <th>Sil</th>
        <th>Güncelle</th>
    </tr>
   @foreach (var item in Model)
   {

        <tr>
            <td>@item.ID</td>
            <td>@item.Baslik</td>
            <td>@item.AltBaslik1</td>
            <td>@item.AltBaslik2</td>
            <td>@item.GNO</td>
            <td>@item.Tarih</td>
            <td><a href="/Egitim/EgitimSil/@item.ID" class="btn btn-outline-primary">Sil</a></td>
            <td><a href="/Egitim/EgitimGetir/@item.ID" class="btn btn-outline-warning">Güncelle</a></td>
        </tr>
    }
</table>
<a href="/Egitim/EgitimEkle/" class="btn btn-danger">Yeni Egitim Ekle</a>

